							}
							Count = 1
							Arc = [180d, 0d]
							Spread = [-10d, 10d]
							InheritVelocity = true
						}
					]
					MediaEffects
					[
						: /BASE_SOUNDS/AudioExterior // sound is here to avoid clips
						{
							DynamicVolume
